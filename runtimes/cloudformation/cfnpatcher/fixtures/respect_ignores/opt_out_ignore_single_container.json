{
  "Resources": {
    "willpatch": {
      "Type": "AWS::ECS::TaskDefinition",
      "Properties": {
        "RequiresCompatibilities": [
          "FARGATE"
        ],
        "Tags": [
          {
            "Key": "antani",
            "Value": "sbiribuda"
          },
          {
            "Key": "kilt-ignore-containers",
            "Value": "nopatch"
          }
        ],
        "ContainerDefinitions": [
          {
            "Name": "app",
            "Image": "busybox",
            "EntryPoint": ["/bin/sh"]
          },
          {
            "Name": "nopatch",
            "Image": "busybox",
            "EntryPoint": ["/bin/sh"]
          }
        ]
      }
    }
  }
}